;***************************************************************************
;MACRO		: readinteger@
;PROPOSITO	: Obtiene valores numВricos desde el teclado
;***************************************************************************

readinteger@ MACRO VALOR1@
LOCAL POS,LISTO
   readstring@ TEMPO@RAL
   ESPACIO@S
   MOV DX,0
   MOV VALOR1@,DX
   MOV CH,TEMPO@RAL[1]
   INC CH
   CMP TEMPO@RAL[2],"-"
   JNE POS
   MOV BX,3
   JMP @F
POS:
   MOV BX,2
@B:
   MOV AL,TEMPO@RAL[BX]
   AND AX,000FH
   ADD AX,VALOR1@
   CMP BL,CH
   JE  @F
   MUL CONVER@T
   INC BX
   MOV VALOR1@,AX
   JMP @B
@F:
   MOV VALOR1@,AX
   CMP TEMPO@RAL[2],"-"
   JNE LISTO
   MOV AX, VALOR1@
   NEG AX
   MOV VALOR1@,AX

LISTO:
   MOV VALOR1@,AX
ENDM

