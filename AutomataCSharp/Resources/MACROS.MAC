;***************************************************************************

readstring@ MACRO VA@LOR
     LEA DX,VA@LOR
     MOV AH,0AH
     INT 21H
     MOV BH,0
     MOV BL,VA@LOR[1]
     MOV AL,"$"
     MOV VA@LOR[2+BX],AL 
ENDM

